<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.assistant.addmember.AddmemberController">
   <children>
      <HBox alignment="CENTER" prefHeight="400.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="250.0" spacing="10.0">
               <children>
                  <JFXTextField fx:id="memberidField" labelFloat="true" layoutX="10.0" layoutY="195.0" promptText="Member ID" />
                  <JFXTextField fx:id="nameField" labelFloat="true" promptText="Name" />
                  <JFXTextField fx:id="emailField" labelFloat="true" layoutX="10.0" layoutY="210.0" promptText="Email" />
                  <JFXTextField fx:id="mobileField" labelFloat="true" layoutX="10.0" layoutY="226.0" promptText="Mobile" />
                  <JFXTextField fx:id="addressField" labelFloat="true" layoutX="10.0" layoutY="201.0" promptText="Address" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <JFXButton fx:id="saveBtn" onAction="#saveMember" prefHeight="36.0" prefWidth="130.0" text="Save" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
